class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int actualsum=0;
        int sum=0;
        for(int i=0;i<=nums.size();i++){
            actualsum+=i;;
        }
        for(int i=0;i<nums.size();i++){
            sum+=nums[i];
        }
        return actualsum - sum;
    }
};
