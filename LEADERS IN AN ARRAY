#include <bits/stdc++.h>
using namespace std;
vector<int> leaders(vector<int>& arr) {
    vector<int> res;
    int n = arr.size();
for (int i = 0; i < n; i++) {
        
        for (int j = i + 1; j < n; j++) {
            if (arr[i] < arr[j])
                break;
        }
        if (j == n) 
            res.push_back(arr[i]);
    }
return res;
}
