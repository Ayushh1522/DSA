#include <iostream>
#include <vector>
using namespace std;

vector<pair<int,int>> findZeroSumSubarrays(vector<int>& arr) {
    vector<pair<int,int>> ans;
    int n = arr.size();

    for (int i = 0; i < n; i++) {
        int sum = 0;
        for (int j = i; j < n; j++) {
            sum += arr[j];
            if (sum == 0) {
                ans.push_back({i, j});
            }
        }
    }
    return ans;
}

